    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compose:up": "PORT=${PORT:=3100} IMAGE=${npm_package_name} IMAGE_TAG=${npm_package_version} docker-compose up --build",
